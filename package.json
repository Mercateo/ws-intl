{
  "name": "@mercateo/ws-intl",
  "version": "1.0.0",
  "description": "Components for passing i18n messages between components/modules",
  "main": "dist-release/index.js",
  "webpack": "src/index.tsx",
  "typings": "dist-release/src/index.d.ts",
