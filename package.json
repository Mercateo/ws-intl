    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "@mercateo/ws": "2.0.0-3",
    "@types/enzyme": "^2.8.0",
    "@types/expect": "^1.20.1",
    "@types/mocha": "^2.2.41",
    "enzyme": "^2.8.2",
    "expect": "^1.20.2",
